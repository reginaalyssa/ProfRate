<div class="container">
  <div class="main-container">
    <p id="notice"><%= notice %></p>
    <div class="row">
      <div class="col-5 mx-auto">
        <h1>New Professor</h1>

        <% if @course_title %>
            <p><%= @course_title %></p>
        <% end %>

        <%= render 'form', professor: @professor %>

        <%= link_to 'Back', professors_path, class: "link-blue" %>
      </div>
    </div>
  </div>
</div>